<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.SendDeliveryActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/send_toolbar"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="?attr/colorPrimary" />

    <Button
        android:id="@+id/button_near"
        style="@style/Base.Widget.AppCompat.Button.Borderless"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/button_order"
        android:layout_marginLeft="95dp"
        android:layout_toStartOf="@+id/button_order"
        android:background="@drawable/leftbutton_background"
        android:onClick="tabChange"
        android:text="附近" />

    <Button
        android:id="@+id/button_order"
        style="@style/Base.Widget.AppCompat.Button.Borderless"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_above="@+id/framelayout_frag"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="9dp"
        android:layout_marginEnd="96dp"
        android:layout_marginLeft="95dp"
        android:background="@drawable/rightbutton_bacaground"
        android:onClick="tabChange"
        android:text="预约" />

    <!--<Button-->
    <!--android:id="@+id/button_order"-->
    <!--style="@style/Base.Widget.AppCompat.Button.Borderless"-->
    <!--android:layout_width="90dp"-->
    <!--android:layout_height="40dp"-->
    <!--android:background="@drawable/rightbutton_bacaground"-->
    <!--android:onClick="tabChange"-->
    <!--android:text="预约记录"-->
    <!--android:layout_marginEnd="96dp"-->
    <!--android:layout_above="@+id/framelayout_frag"-->
    <!--android:layout_alignParentEnd="true"-->
    <!--android:layout_marginBottom="9dp" />-->


    <FrameLayout
        android:id="@+id/framelayout_frag"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"

        android:layout_below="@+id/send_toolbar" />


</RelativeLayout>
